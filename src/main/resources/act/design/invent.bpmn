  <process id="invent" name="新产品研发流程" isExecutable="true">
    <startEvent id="startevent1" name="Start"></startEvent>
    <userTask id="发布新产品生产订单" name="发布新产品生产订单" activiti:candidateUsers="${manager}">
      <documentation>每个经理都可以发布新产品订单</documentation>
    </userTask>
    <userTask id="接受生产订单" name="接受生产订单" activiti:candidateUsers="${producters}">
      <documentation>使用用户组任务</documentation>
    </userTask>
    <userTask id="完成生产订单" name="完成生产订单" activiti:candidateUsers="${producters}">
      <documentation>使用用户组任务</documentation>
    </userTask>
    <userTask id="审核产品" name="审核产品" activiti:candidateUsers="${auditors}">
      <documentation>使用用户组任务</documentation>
    </userTask>
    <sequenceFlow id="flow1" sourceRef="发布新产品生产订单" targetRef="接受生产订单"></sequenceFlow>
    <sequenceFlow id="flow2" sourceRef="接受生产订单" targetRef="完成生产订单"></sequenceFlow>
    <sequenceFlow id="flow3" sourceRef="完成生产订单" targetRef="审核产品"></sequenceFlow>
    <exclusiveGateway id="exclusivegateway1" name="Exclusive Gateway"></exclusiveGateway>
    <sequenceFlow id="flow4" sourceRef="审核产品" targetRef="exclusivegateway1"></sequenceFlow>
    <userTask id="登记入库" name="登记入库" activiti:candidateUsers="${producters}">
      <documentation>使用用户组任务</documentation>
    </userTask>
    <receiveTask id="订单成功结束" name="订单成功结束"></receiveTask>
    <sequenceFlow id="flow10" name="审核成功" sourceRef="exclusivegateway1" targetRef="登记入库">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${auditOk == true}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow11" sourceRef="登记入库" targetRef="订单成功结束"></sequenceFlow>
    <sequenceFlow id="flow13" sourceRef="startevent1" targetRef="发布新产品生产订单"></sequenceFlow>
    <receiveTask id="订单失败结束" name="订单失败结束"></receiveTask>
    <sequenceFlow id="flow16" name="审核失败" sourceRef="exclusivegateway1" targetRef="订单失败结束">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${auditOk == false}]]></conditionExpression>
    </sequenceFlow>
    <endEvent id="endevent1" name="End"></endEvent>
    <sequenceFlow id="flow17" sourceRef="订单失败结束" targetRef="endevent1"></sequenceFlow>
    <sequenceFlow id="flow18" sourceRef="订单成功结束" targetRef="endevent1"></sequenceFlow>
  </process>
