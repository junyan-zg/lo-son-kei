  <process id="invent" name="新产品研发流程" isExecutable="true">
    <startEvent id="startevent1" name="Start"></startEvent>
    <userTask id="usertask1" name="发布新产品生产订单" activiti:assignee="${manager}">
      <documentation>每个经理都可以发布新产品订单</documentation>
    </userTask>
    <userTask id="usertask2" name="接受生产订单" activiti:candidateUsers="${producters}">
      <documentation>使用用户组任务</documentation>
    </userTask>
    <userTask id="usertask3" name="完成生产订单" activiti:candidateUsers="${producters}">
      <documentation>使用用户组任务</documentation>
    </userTask>
    <userTask id="usertask4" name="审核产品" activiti:candidateUsers="${auditors}">
      <documentation>使用用户组任务</documentation>
    </userTask>
    <sequenceFlow id="flow1" sourceRef="usertask1" targetRef="usertask2"></sequenceFlow>
    <sequenceFlow id="flow2" sourceRef="usertask2" targetRef="usertask3"></sequenceFlow>
    <sequenceFlow id="flow3" sourceRef="usertask3" targetRef="usertask4"></sequenceFlow>
    <exclusiveGateway id="exclusivegateway1" name="Exclusive Gateway"></exclusiveGateway>
    <sequenceFlow id="flow4" sourceRef="usertask4" targetRef="exclusivegateway1"></sequenceFlow>
    <endEvent id="endevent1" name="End"></endEvent>
    <userTask id="usertask5" name="重开订单" activiti:assignee="${manager}"></userTask>
    <exclusiveGateway id="exclusivegateway2" name="Exclusive Gateway"></exclusiveGateway>
    <sequenceFlow id="flow5" sourceRef="usertask5" targetRef="exclusivegateway2"></sequenceFlow>
    <sequenceFlow id="flow6" name="是" sourceRef="exclusivegateway2" targetRef="usertask2">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${createOrdersAgain == true}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow9" name="否" sourceRef="exclusivegateway2" targetRef="endevent1">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${createOrdersAgain == false}]]></conditionExpression>
    </sequenceFlow>
    <userTask id="usertask6" name="登记入库" activiti:candidateUsers="${producters}">
      <documentation>使用用户组任务</documentation>
    </userTask>
    <receiveTask id="receivetask2" name="订单成功结束"></receiveTask>
    <sequenceFlow id="flow10" name="审核成功" sourceRef="exclusivegateway1" targetRef="usertask6">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${auditOk == true}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow11" sourceRef="usertask6" targetRef="receivetask2"></sequenceFlow>
    <sequenceFlow id="flow12" sourceRef="receivetask2" targetRef="endevent1"></sequenceFlow>
    <sequenceFlow id="flow13" sourceRef="startevent1" targetRef="usertask1"></sequenceFlow>
    <receiveTask id="receivetask3" name="订单失败结束"></receiveTask>
    <sequenceFlow id="flow16" name="审核失败" sourceRef="exclusivegateway1" targetRef="receivetask3">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${auditOk == false}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow17" sourceRef="receivetask3" targetRef="usertask5"></sequenceFlow>
  </process>
